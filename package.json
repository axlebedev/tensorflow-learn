    "flow-bin": "^0.151.0",
    "jest": "^27.0.1",
    "lint-staged": "^11.0.0",
    "markdown-it": "^12.0.6",
    "mkdirp": "^1.0.4",
    "pixrem": "^5.0.0",
    "pleeease-filters": "^4.0.0",
